def twoSum(nums, target):
    num_index = {}
    for i, num in enumerate(nums):
        complement = target - num
        if complement in num_index:
            return[num, complement]
        num_index[num] = i
        
    return None


nums = [3, 2, 4]
target = 6
result = twoSum(nums, target)
print(result)
